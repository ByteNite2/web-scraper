{
  "id": "web-scraper-dibek-fixed",
  "description": "Web scraper with Dibek's fixes - v0.5 partitioner + v0.6 scraper",
  "app": "scraper-engine",
  "partitioner": "fanout-urls", 
  "assembler": "data-assembler",
  "default_params": {
    "partitioner": {
      "app_id": "fanout-urls",
      "version": "0.5",
      "parameters": {
        "urls": [
          "https://www.amazon.com/s?k=echo+dot&ref=nb_sb_noss",
          "https://www.amazon.com/s?k=kindle+paperwhite&ref=nb_sb_noss",
          "https://www.amazon.com/s?k=fire+tv+stick&ref=nb_sb_noss"
        ],
        "chunk_size": 1
      }
    },
    "app": {
      "app_id": "scraper-engine",
      "version": "0.6",
      "parameters": {
        "timeout": 30000,
        "headless": true,
        "delay_between_requests": 2
      }
    },
    "assembler": {
      "app_id": "data-assembler",
      "version": "0.2",
      "parameters": {}
    }
  }
}
